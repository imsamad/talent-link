{
  "exclude": ["node_modules"],
  "extends": "@repo/typescript-config/nextjs.json",
  "compilerOptions": {
    "outDir": "dist",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "paths": {
    "@/*": ["app/*"]
  },
  "include": [
    "next.config.tss",
    "next.config.js",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app"
  ]
}
